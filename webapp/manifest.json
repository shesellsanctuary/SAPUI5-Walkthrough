{
	"_version": "1.3.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "com.sap.CloudSCAME.SAPUI5-walkthrough",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"ach": "XX",
		"dataSources": {
			"invoiceRemote": {
			"uri": "https://services.odata.org/V2/Northwind/Northwind.svc/",
			"type": "OData",
			"settings": {
				"odataVersion": "2.0"
			}
			}
		}
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize",
      "sap_belize_plus"
		]
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"rootView": {
			"viewName": "com.sap.CloudSCAME.SAPUI5-walkthrough.view.App",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
		  "minUI5Version": "1.30",
		  "libs": {
			"sap.m": {}
		  }
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.sap.CloudSCAME.SAPUI5-walkthrough.i18n.i18n"
				}
			},
			"invoice": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "Invoices.json"
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.sap.CloudSCAME.SAPUI5-walkthrough.view",
				"controlId": "idAppControl",
				"controlAggregation": "pages",
				"async": true
			},
			"routes": [
            ],
			"targets": {
			}
		},
		"resources": {
		  "css": [
			{
			  "uri": "css/style.css"
			}
		  ]
		}
	}
}
